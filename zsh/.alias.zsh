alias \
    g='git' \
    cl='clear' \
    cp='cp -iv' \
    mv='mv -iv' \
    rm='rm -Iv' \
    mkd='mkdir -pv' \
    ls='ls --color=auto --group-directories-first' \
    ll='ls -lh' \
    la='ls -A' \
    lla='ls -Alh' \
    grep='grep --colour=auto' \
    diff='diff --colour=auto' \
    df='df -h' \
    free='free -m' \
    -='cd -' \
    ..='cd ..' \
    ...='cd ../..' \
    ....='cd ../../..' \
    .....='cd ../../../..' \
    ......='cd ../../../../..' \
