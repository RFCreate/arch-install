{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} :: {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#2f2f2f",
          "template": "\u256d\u2500"
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#2f2f2f",
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}\ue62a \uebc6 at {{ end }}{{ .Icon }}"
        },
        {
          "type": "root",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffff66",
          "background": "#2f2f2f",
          "template": "<#7a7a7a> \ue0b1</> \uf0ad"
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#469fdf",
          "background": "#2f2f2f",
          "template": "<#7a7a7a> \ue0b1</> {{ if eq .Path \"~\" }}\uf015{{ else }}\uf07c{{ end }}  {{ .Path }}",
          "properties": {
            "home_icon": "~",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "#2dc915",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#b388ff{{ end }}",
            "{{ if gt .Behind 0 }}#b388ff{{ end }}"
          ],
          "background": "#2f2f2f",
          "template": "<#7a7a7a> \ue0b1</> {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }}",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#2f2f2f",
          "background": "#2f2f2f",
          "template": "_"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#2f2f2f",
          "template": "\ue0b0"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#2f2f2f",
          "template": "\ue0b2"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#2f2f2f",
          "background": "#2f2f2f",
          "template": "_"
        },
        {
          "type": "python",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#e1e73b",
          "background": "#2f2f2f",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 <#7a7a7a> \ue0b3</> ",
          "properties": {
            "home_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#2f2f2f",
          "template": "{{ .FormattedMs }} \uf252 <#7a7a7a> \ue0b3</> ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#408434",
          "background": "#2f2f2f",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .CurrentDate | date .Format }} \uf017 "
        }
      ],
      "overflow": "hide"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "diamond",
          "foreground": "#2dc915",
          "foreground_templates": [
            "{{ if .Error }}#e91e63{{ end }}"
          ],
          "background": "#2f2f2f",
          "leading_diamond": "\u2570\u2500\ue0b6",
          "trailing_diamond": "\ue0b4\u2500\ue285",
          "template": " {{ if eq .Code 0 }}\uf00c{{ else }}\uf467{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "newline": true
    }
  ]
}
